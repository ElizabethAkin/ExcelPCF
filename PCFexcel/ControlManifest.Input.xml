    <property name="entitySchemaName" display-name-key="Entity Schema Name" description-key="Entity Schema Name" of-type="SingleLine.Text" usage="input" required="true" />
    <property name="entityCollectionSchemaName" display-name-key="Entity Schema Collection Name" description-key="Entity Schema Collection Name" of-type="SingleLine.Text" usage="input" required="true" />
    <feature-usage>
      <uses-feature name="Utility" required="true" />
      <uses-feature name="WebAPI" required="true" />
    </feature-usage>
    <resources>
      <code path="index.ts" order="1" />
      <css path="css/PCFexcel.css" order="1" />
    </resources>
  </control>
</manifest>